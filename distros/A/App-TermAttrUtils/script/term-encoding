#!perl

our $DATE = '2020-06-10'; # DATE
our $DIST = 'App-TermAttrUtils'; # DIST
our $VERSION = '0.006'; # VERSION

use 5.010001;
use strict;
use warnings;
use Term::Encoding;
say Term::Encoding::get_encoding;

# PODNAME: term-encoding
# ABSTRACT: Print terminal encoding according to Term::Encoding

__END__

