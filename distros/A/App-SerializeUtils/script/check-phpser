#!perl

use 5.010;
use strict;
use warnings;

use Data::Dump;
use PHP::Serialization qw(unserialize);

our $VERSION = '0.164'; # VERSION

eval {
    undef $/;
    unserialize(scalar <>);
};
die $@ if $@;

1;
# ABSTRACT: Convert PHP to Data::Dump serialization
# PODNAME: check-phpser

__END__

=pod

=encoding UTF-8

=head1 NAME

check-phpser - Convert PHP to Data::Dump serialization

