#!/bin/sh -e

libdir="/Users/tomvanderwoerdt/code/perl-AnyEvent-YACurl/AnyEvent-YACurl/nghttp2-src/lib"
if [ -d "$libdir/.libs" ]; then
    libdir="$libdir/.libs"
fi

export CGO_CFLAGS="-I/Users/tomvanderwoerdt/code/perl-AnyEvent-YACurl/AnyEvent-YACurl/nghttp2-src/lib/includes -I/Users/tomvanderwoerdt/code/perl-AnyEvent-YACurl/AnyEvent-YACurl/nghttp2-src/lib/includes"
export CGO_LDFLAGS="-L$libdir"
export LD_LIBRARY_PATH="$libdir"
export GODEBUG=cgocheck=0
"$@"
