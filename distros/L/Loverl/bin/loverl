#!/usr/bin/env perl
# ABSTRACT: Loverl application script
# PODNAME: Loverl

use v5.36;

use FindBin qw($Bin);
use lib "$Bin/../lib";
use Loverl;

my %arg = ( show_version_cmd => 1, );
my $cmd = Loverl->new( \%arg );
$cmd->run;

__END__

