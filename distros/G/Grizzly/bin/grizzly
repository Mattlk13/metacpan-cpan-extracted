#!/usr/bin/env perl
# ABSTRACT: Grizzly application script
# PODNAME: Grizzly

use strict;
use warnings;

use FindBin qw($Bin);
use lib "$Bin/../lib";
use Grizzly;

my %arg = ( show_version_cmd => 1, );
my $cmd = Grizzly->new( \%arg );
$cmd->run;

__END__

=pod

=encoding UTF-8

=head1 NAME

Grizzly - Grizzly application script

