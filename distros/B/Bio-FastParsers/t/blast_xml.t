use Test::Most;

use autodie;
use feature qw(say);

use List::AllUtils;
use Path::Class qw(file);

use Bio::FastParsers;


my $class = 'Bio::FastParsers::Blast::Xml';

{
    # open and parse BLAST report in XML format
    my $infile = file('test', 'empty.xml');
    ok my $report = $class->new( file => $infile ), 'Blast::XML constructor';
    isa_ok $report, $class, $infile;

    # get main container
    my $bo = $report->blast_output;
    # TODO: check exception
}

