BEGIN { unshift( @INC, abs_path('./t/lib') ) }
use Bio::Roary::CommandLine::RoaryPostAnalysis;

Bio::Roary::CommandLine::RoaryPostAnalysis->new(args => \@ARGV, script_name => $0)->run;

__END__

