# ifdef __CPP__TYPES
int main() {
    number foo = abs (3);
    number bar = -3;
    print "have $foo = " << foo << endl;
    print "have $bar = " << bar << endl;
    number result = (abs (bar)) * 23 + 1;
    result += abs (bar) * 10;
    print "have $result = " << result << endl;
    return 0;
}
