void MySubclass02LowRPerlNew::triple_bax_save() {
    this->bax = this->bax * 3;
    return;
}

integer MySubclass02LowRPerlNew::triple_bax_return() {
    return this->bax * 3;
}

integer MySubclass02LowRPerlNew::add_bax_return(integer addend) {
    if ( addend < 10 ) {
        return this->bax + addend;
    }
    return this->bax + 3;
}

integer MySubclass02LowRPerlNew::subtract_bax_return(integer subtrahend) {
    if ( subtrahend < 10 ) {
        return this->bax - subtrahend;
    }
    return this->bax - 3;
}

integer MySubclass02LowRPerlNew::multiply_bax_return(integer multiplier) {
    if ( multiplier < 10 ) {
        return this->bax * multiplier;
    }
    return this->bax * 3;
}

integer MySubclass02LowRPerlNew::multiply_multiply_bax_return(integer multiplier, integer multiplier2) {
    if ( multiplier < 10 ) {
        return this->bax * multiplier * multiplier2;
    }
    return this->bax * 3 * multiplier2;
}

