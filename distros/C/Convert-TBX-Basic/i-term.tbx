      <termEntry id="aid256">
        <descrip type="subjectField">7.1 Terminography (representation of term entries)</descrip>
        <note>Approved</note>
        <langSet xml:lang="en" id="cid384">
          <descrip type="definition">concept in one language that comprises the same characteristics as that covered by a given concept in another language</descrip>
          <admin type="source">ISO 26162/FDIS</admin>
          <descrip type="definition">Alt.def: </descrip>
          <admin type="source"/>
          <note/>
          <note/>
          <note/>
          <transacGrp>
            <transac type="transactionType">origination</transac>
            <transacNote type="responsibility">Kara Warburton (kara)</transacNote>
            <date>2007-05-06 18:16:05</date>
