      <termEntry id="aid1512">
        <descrip type="subjectField">7 Data representation</descrip>
        <note/>
        <langSet xml:lang="en" id="cid1964">
          <descrip type="definition">class of dialogue acts that are concerned with a particular aspect of communication, corresponding to a particular category of semantic content</descrip>
