      <termEntry id="aid1515">
        <descrip type="subjectField">7 Data representation</descrip>
        <note/>
        <langSet xml:lang="en" id="cid1967">
          <descrip type="definition">relation between a given dialogue act and a preceding dialogue act on which the semantic content of the given dialogue act depends due to its communicative function</descrip>
          <admin type="source">ISO/DIS 24617-2</admin>
