            <transac type="transactionType">modification</transac>
            <transacNote type="responsibility">Gwyneth Buck (gwyneth)</transacNote>
            <date>2008-11-17 16:10:32</date>
          </transacGrp>
          <tig>
            <term id="tid1840">international terminology standard</term>
            <termNote type="partOfSpeech">Noun</termNote>
            <termNote type="termType"/>
            <termNote type="administrativeStatus">Proposed</termNote>
            <termNote type="pronunciation"/>
            <note/>
            <note/>
            <descrip type="context">In most cases, the definitions given in international terminology standards are applicable to local conditions, or may be made applicable by minor modifications, to ensure equivalency with the international terminolology standard.</descrip>
            <admin type="source">ISO/DIS 10241-2</admin>
          </tig>
        </langSet>
      </termEntry>
