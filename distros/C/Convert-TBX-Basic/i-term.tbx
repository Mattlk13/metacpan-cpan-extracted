            <transac type="transactionType">modification</transac>
            <transacNote type="responsibility">Hanne Erdman Thomsen (het)</transacNote>
            <date>2010-04-15 14:30:24</date>
          </transacGrp>
          <tig>
            <term id="tid1027">neology</term>
            <termNote type="partOfSpeech"/>
            <termNote type="termType"/>
            <termNote type="administrativeStatus"/>
            <termNote type="pronunciation"/>
            <note/>
            <note/>
            <descrip type="context">The neology consists in creation of a new term (unedited form), a new meaning in a form already in the language, or in borrowing of a term from another language in another field of knowledge.</descrip>
            <admin type="source">ISO/DTR 22134</admin>
          </tig>
        </langSet>
      </termEntry>
      <termEntry id="aid524">
        <descrip type="subjectField">4 Socioterminology and cultural diversification</descrip>
